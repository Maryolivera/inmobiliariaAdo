@{
    ViewData["Title"] = "Inicio";
}

<h1>Inicio</h1>

<h3>Inmuebles disponibles hoy: @Model.Count</h3>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Direcci√≥n</th>
            <th>Tipo de Inmueble</th>
            <th>Uso</th>
            <th>Ambientes</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Direccion</td>
            <td>@item.TipoNombre</td>
